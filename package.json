    "promisify": "^0.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "should-send-same-site-none": "^2.0.5",
    "swagger-ui-express": "^4.3.0",
